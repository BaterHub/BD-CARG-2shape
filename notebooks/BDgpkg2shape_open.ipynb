        "configs = {\n",
        " 'ST010Point': { 'search':['ST010Point','main.ST010Point'], 'output':'geomorfologia_punti.shp',\n",
        "   'fields': {'Pun_Gmo':'Pun_Gmo','Tipo':'Tipo_Gmrf','Tipologia':'Tipologia','Stato':'Stato','Direzio':'Direzione'},\n",
